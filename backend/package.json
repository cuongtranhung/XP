{
  "name": "fullstack-auth-backend",
  "version": "2.0.0",
  "description": "Backend API for fullstack authentication system",
  "main": "dist/server.js",
  "scripts": {
    "start": "node dist/server.js",
    "dev": "tsx watch src/server.ts",
    "build": "tsc",
    "migrate": "node scripts/migrate.js",
    "setup-db": "node scripts/setup-database.js",
    "db:migrate": "tsx src/database/migrate.ts",
    "db:setup": "npm run db:migrate",
    "db:test": "tsx src/database/test-connection.ts",
    "db:generate-schema": "node scripts/generate-schema-script.js",
    "formbuilder:setup": "tsx src/modules/dynamicFormBuilder/setup.ts",
    "formbuilder:migrate": "tsx src/modules/dynamicFormBuilder/migrations/migrate.ts migrate",
    "formbuilder:rollback": "tsx src/modules/dynamicFormBuilder/migrations/migrate.ts rollback",
    "formbuilder:reset": "tsx src/modules/dynamicFormBuilder/migrations/migrate.ts reset",
    "formbuilder:status": "tsx src/modules/dynamicFormBuilder/migrations/migrate.ts status",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:group-management": "jest --testPathPattern=group --verbose",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "test:load": "tsx src/scripts/run-websocket-optimization-load-test.ts",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix"
  },
  "keywords": [
    "authentication",
    "express",
    "typescript",
    "jwt"
  ],
  "author": "Your Name",
  "license": "MIT",
